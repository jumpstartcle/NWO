  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SalesForce</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@OppId">
            <Value>=Parameters!OppId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select c.Name "AssignedTo"
, acc.Name "AssignedOrg"
, act.ActivityDate "DueDate"
, act.Subject
, act.Comments
, act.Status
, act.Id "ActId"
, "Overdue" = case
 when act.activitydate &lt; getdate() then 1 else 0 end
from (select ID, activitydate, whatid, subject, status, CONVERT(varchar(1000), description) "Comments", ownerid
from Task
where Status not in ('Completed','Canceled')
union
select ID, activitydate, whatid, subject, 'Future Event', CONVERT(varchar(1000), description) "Comments", ownerid
from event
where ActivityDate &gt; GETDATE()) act
inner join [User] u on act.OwnerId = u.Id
inner join Contact c on c.Id = u.User_to_Contact_lookup__c
inner join Account acc on c.AccountId = acc.id
where act.WhatId = @OppId</CommandText>
      </Query>
      <Fields>
        <Field Name="AssignedTo">
